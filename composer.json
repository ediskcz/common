{
    "name": "edisk/common",
    "description": "eDisk common lib",
    "keywords": [
        "eDisk",
        "php"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Edisk\\Common\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Edisk\\Common\\": "tests/src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "egulias/email-validator": "^3.2",
        "geoip2/geoip2": "^2.13",
        "matomo/device-detector": "^6.1",
        "symfony/http-foundation": "^6.0",
        "symfony/lock": "^6.0",
        "symfony/validator": "^6.0",
        "doctrine/orm": "^2.19",
        "symfony/cache": "^6.0",
        "beberlei/doctrineextensions": "^1.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.6"
    }
}
