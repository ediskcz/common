{
    "name": "edisk/common",
    "description": "eDisk common lib",
    "keywords": [
        "eDisk",
        "php"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Edisk\\Common\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Edisk\\Common\\": "tests/src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "matomo/device-detector": "^6.1",
        "symfony/validator": "^6.0",
        "egulias/email-validator": "^3.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.6"
    }
}
